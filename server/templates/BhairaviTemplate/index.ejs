<!DOCTYPE html>
<html lang="en">
<head profile="http://www.w3.org/2005/10/profile">
<!-- Meta Stuff for the Browser to render correctly -->
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">

<!-- Miscellaneous client-side data -->
<script type="application/javascript">
window.developmentMode = <%= renderConfig.developmentMode %>;
</script>

<!-- Title: Will change based on the actual slogan or whatever -->
<title><%= renderConfig.title%></title>

<!-- Google Fonts for the text on the Web Application -->
<link as="font" crossorigin="crossorigin" href="//fonts.googleapis.com/css?family=Keania+One" rel="stylesheet">
<link as="font" crossorigin="crossorigin" href="//fonts.googleapis.com/css?family=Noto+Sans:400,400i,700,700i" rel="stylesheet">

<!-- HTML Normalize CSS Styles -->
<link href="//cdnjs.cloudflare.com/ajax/libs/normalize/7.0.0/normalize.min.css" rel="stylesheet" type="text/css" />

<!-- Font Awesome, for the Glyphs -->
<link href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css" />

<!-- jQuery UI CSS Styles -->
<link href="//cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css" rel="stylesheet" type="text/css" />

<!-- Bootstrap CSS Styles -->
<link href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap-theme.min.css" rel="stylesheet" type="text/css" />

<!-- jQuery Plugins, overridden by AdminLTE -->
<link href="//cdnjs.cloudflare.com/ajax/libs/datatables/1.10.15/css/jquery.dataTables.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.4.0/fullcalendar.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/iCheck/1.0.2/skins/all.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/jquery.inputmask/3.3.4/css/inputmask.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/ion-rangeslider/2.2.0/css/ion.rangeSlider.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/ion-rangeslider/2.2.0/css/ion.rangeSlider.skinModern.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" rel="stylesheet" type="text/css" />

<!-- Bootstrap Plugins, overridden by AdminLTE -->
<link href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/9.8.0/css/bootstrap-slider.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-colorpicker/2.5.1/css/bootstrap-colorpicker.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/datatables/1.10.15/css/dataTables.bootstrap.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.7.0/css/bootstrap-datepicker3.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-daterangepicker/2.1.25/daterangepicker.min.css" rel="stylesheet" type="text/css" />
<link href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-timepicker/0.5.2/css/bootstrap-timepicker.min.css" rel="stylesheet" type="text/css" />

<!-- Web Application AdminLTE Theme CSS Styles -->
<link href="/BhairaviTemplate/css/AdminLTE.css" rel="stylesheet" type="text/css" />
<link href="/BhairaviTemplate/css/skins/skin-black-light.css" rel="stylesheet" type="text/css" />

<!-- Overriding styles -->
<style type="text/css">
.skin-black-light .main-header > .logo {
	border-right:0px;
	background-color: #fff;
}

.skin-black-light .content-wrapper {
	border-left: 0px;
}

.skin-black-light .main-footer {
	margin-left:0px;
	border-left: 0px;
}
/*
.skin-black-light .main-footer a {
	color: #333;
}
*/
.wrapper {
	min-height:0px;
	height: auto;
}

.content-wrapper {
	min-height:100px;
	background-color:transparent;

	margin-left:0px;
	border-left: 0px;
}

.modal-open {
	overflow-y: auto;
}

.box {
	margin: 10px 0px;
	border-radius: 0px;
}
</style>
</head>
<body class="skin-black-light">
<!-- ES5 / ES6 Shims / Shams before anything else -->
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/es5-shim/4.5.7/es5-shim.min.js"></script>
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/es5-shim/4.5.7/es5-sham.min.js"></script>
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/json3/3.3.2/json3.min.js"></script>
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/es6-shim/0.35.3/es6-shim.min.js"></script>
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/es6-shim/0.35.3/es6-sham.min.js"></script>

<!-- loader.js - Basic require / define implementation -->
<script type="application/javascript" src="/BhairaviTemplate/bower_components/loader.js/lib/loader/loader.js"></script>

<!-- Fastclick for removing touch event delays on tablets / phones -->
<!-- script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/fastclick/1.0.6/fastclick.min.js"></script -->

<!-- MomentJS - for easier date/time manipulation -->
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js"></script>
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.13/moment-timezone-with-data.min.js"></script>

<!-- jQuery JS Library - everything depends on it -->
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>

<!-- jQuery Plugins -->
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/js-cookie/2.1.3/js.cookie.min.js"></script>

<script type="application/javascript">
$.ajaxSetup({
	'cache': <%= !renderConfig.developmentMode %>,
	'xhrFields': {
		'withCredentials': true
	}
});
</script>

<!-- Bootstrap JS Library - the base theme stuff -->
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>

<!-- AdminLTE widgets - the base theme stuff -->
<script type="application/javascript">
$.AdminLTE = {
	'options': {
		'animationSpeed': 200,
		'enableBoxWidget': true,

		'boxWidgetOptions': {
			'boxWidgetIcons': {
				'collapse': 'fa-minus',
				'open': 'fa-plus',
				'remove': 'fa-times'
			},

			'boxWidgetSelectors': {
				'remove': '[data-widget="remove"]',
				'collapse': '[data-widget="collapse"]'
			}
		}
	}
};
</script>
<script type="application/javascript" src="/BhairaviTemplate/js/adminlte.js"></script>

<!-- Ember.js, for the client-side MVC Framework -->
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/ember.js/2.12.0/ember.min.js"></script>

<!-- Ember Data - in-browser data model -->
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/ember-data.js/2.12.1/ember-data.min.js"></script>

<!-- Ember.js shims, to reduce size -->
<script type="application/javascript" src="/BhairaviTemplate/bower_components/ember-cli-shims/vendor/ember-cli-shims/app-shims.js"></script>

<!-- Ember Template Compiler - convert scripts to HTMLBars -->
<script type="application/javascript" src="//cdnjs.cloudflare.com/ajax/libs/ember.js/2.12.0/ember-template-compiler.js"></script>

<!-- Ember Load Initializers - start the App in a happy place -->
<script type="application/javascript" src="/BhairaviTemplate/js/ember-load-initializers.js"></script>

<!-- Ember Resolver - use the defines... -->
<script type="application/javascript" src="/BhairaviTemplate/js/ember-resolver.js"></script>

<!-- Primus - realtime websocket connection stuff -->
<script type="application/javascript" src="/websockets/primus.js"></script>

<!-- App.js - script to init/start the Twy'r Ember App -->
<script type="application/javascript" src="/BhairaviTemplate/js/app.js"></script>

<script type="text/x-handlebars">
<div class="wrapper" style="background-color:#d2d6de;">
	<!-- Header -->
	<header class="main-header">
		{{#link-to "application" class="logo"}}
			<span class="logo-lg">
				<img src="/Applications/logo.png" style="max-width:100%; max-height:40px;" />
			</span>
		{{/link-to}}

		<nav class="navbar navbar-static-top" role="navigation">
			<div class="navbar-custom-menu">
				<ul class="nav navbar-nav">
					{{component "application-chooser-widget" layout="application-chooser-widget" controller-action="controller-action"}}
					{{component "profile-widget" layout="profile-widget" controller-action="controller-action"}}
					{{component "logout-widget" layout="logout-widget" controller-action="controller-action"}}
				</ul>
			</div>
		</nav>
	</header>

	<!-- Position: error -->
	<div id="template-error-position" style="position:fixed; top:10px; left:25%; min-width:50%; width:50%; max-width:50%; z-index:9999; text-align:center; display:inline-block;">
		<div id="template-status-message" class="callout" style="display:none; border-radius:0px; width:100%;">
			<span></span>
		</div>

		<div id="template-error-message" class="callout callout-danger" style="display:none; border-radius:0px; width:100%;">
		{{#if errorModel.errors.length}}
			{{#each errorModel.errors as |error|}}
				<span>{{error.detail}}</span>
				<br />
			{{/each}}
		{{/if}}

		{{#if errorModel.adapterError}}
			<span>{{errorModel.adapterError.message}}</span>
		{{/if}}
		</div>
	</div>

	<!-- Main Area -->
	<div class="content-wrapper">
		{{outlet}}
	</div>

	<!-- Footer -->
	<footer class="main-footer">
		<div class="pull-right hidden-xs" style="font-weight:600;">
			Copyright&copy; <%= renderConfig.baseYear %>-<%= renderConfig.currentYear %> {{#link-to "application"}}Twy&apos;r Framework{{/link-to}}. All rights reserved.
		</div>
		<p>&nbsp;</p>
	</footer>
</div>
</script>

<!-- Ember Helpers -->
<script type="application/javascript">
<%- Helpers %>
</script>

<!-- Ember Component HTMLs -->
<%- ComponentHTMLs %>

<!-- Ember Components -->
<script type="application/javascript">
<%- Components %>
</script>

<!-- Ember Models -->
<script type="application/javascript">
<%- Models %>
</script>

<!-- Ember Controllers -->
<script type="application/javascript">
<%- Controllers %>
</script>

<!-- Ember Services -->
<script type="application/javascript">
<%- Services %>
</script>

<!-- Ember Route Handlers -->
<script type="application/javascript">
<%- RouterHandlers %>
</script>

<script type="application/javascript">
require('twyr-webapp/application')['default'];
</script>
</body>
</html>
